	<queryString>
		<![CDATA[SELECT
     *
FROM
     `DetalleFactura` DetalleFactura INNER JOIN `DetalleFactura` Factura ON DetalleFactura.`numeroFactura` = Factura.`numeroFactura`
     INNER JOIN `join` Clientes ON Factura.`codigoCliente` = Clientes.`codigoCliente`
     INNER JOIN `join` Productos ON DetalleFactura.`codigoProducto` = Productos.`codigoProducto`
WHERE
     Factura.numeroFactura = $P{variableId}]]>
	</queryString>
