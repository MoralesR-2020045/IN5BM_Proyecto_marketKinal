<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SplitMenuButton?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="org.rammiromorales.controller.ProductoViewController">
   <children>
      <Button layoutX="35.0" layoutY="32.0" mnemonicParsing="false" text="Agregar" />
      <Button layoutX="39.0" layoutY="80.0" mnemonicParsing="false" text="Eliminar" />
      <Button layoutX="39.0" layoutY="129.0" mnemonicParsing="false" text="Editar" />
      <Button layoutX="39.0" layoutY="169.0" mnemonicParsing="false" text="Button" />
      <SplitMenuButton layoutX="400.0" layoutY="57.0" mnemonicParsing="false" text="SplitMenuButton">
        <items>
            <Menu mnemonicParsing="false" text="Modulo ">
              <items>
                <MenuItem fx:id="btnMenuPrincipal" mnemonicParsing="false" onAction="#handleButtonAction" text="Menu Principal" />
                  <MenuItem fx:id="btnClientes" mnemonicParsing="false" onAction="#handleButtonAction" text="Clientes" />
              </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Acerca de">
              <items>
                <MenuItem fx:id="btnProgramador" mnemonicParsing="false" onAction="#handleButtonAction" text="Programador" />
              </items>
            </Menu>
        </items>
      </SplitMenuButton>
   </children>
</AnchorPane>
